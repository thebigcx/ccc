#define PRINT

struct Structure
{
    x: int32, y: int32
};

var public x: int32;

fn public main(argc: int32, argv: int8**)
{
    var st: Structure[2];
    st[2].x = 100;

    //var x: int32[4];
    //x[2] = 10 + 2;
    printf("%d\n", st[2].x);

    for (x = 10; x > -2; x--)
    {
#ifdef PRINT
        printf("%d\n", x);
#endif
    }
}